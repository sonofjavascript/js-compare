{
  "name": "@sonofjavascript/js-compare",
  "author": "Albert Pérez Farrés",
  "version": "1.0.4",
  "description": "Lightweight and simple Javascript tool to compare equality with objects, arrays and other Javascript object types.",
  "main": "lib/index.js",
  "scripts": {
    "build": "BABEL_ENV=production babel src -d lib",
    "test:dev": "jest --watchAll",
    "test": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "linter-fix": "standard --fix",
    "linter-check": "standard"
  },
  "keywords": [
    "json",
    "javascript",
    "object",
    "objects",
    "compare",
    "equallity",
    "equal",
    "equals",
    "check",
    "empty"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.js",
      "!**/**/index.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageDirectory": "./coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-minify": "^0.5.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "core-js": "^3.3.1"
  }
}
